{
  "name": "tristan-ui-timeline",
  "version": "0.4.13",
  "description": "Advanced Timeline component for Tristan UI - powerful data visualization",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./dist/style.css": "./dist/tristan-ui-timeline.css",
    "./style.css": "./dist/tristan-ui-timeline.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "keywords": [
    "react",
    "timeline",
    "data-visualization",
    "gantt-chart",
    "ui-component",
    "typescript"
  ],
  "author": "wenchuanlilizhao",
  "license": "MIT",
  "homepage": "https://github.com/wenchuanlilizhao/tristan-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenchuanlilizhao/tristan-ui.git",
    "directory": "react/packages/timeline"
  },
  "bugs": {
    "url": "https://github.com/wenchuanlilizhao/tristan-ui/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "vite build --mode library && tsc -p tsconfig.json --emitDeclarationOnly && cp dist/src/index.d.ts dist/index.d.ts",
    "clean": "rm -rf dist",
    "dev": "vite build --mode library --watch"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "tristan-ui-core": "^0.4.12"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.3.3",
    "sass": "^1.81.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.1"
  }
}
