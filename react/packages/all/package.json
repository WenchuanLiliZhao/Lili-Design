{
  "name": "tristan-ui",
  "version": "0.4.8",
  "description": "Complete Tristan UI component library - modern React design system",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./dist/style.css": "./dist/tristan-ui.css",
    "./style.css": "./dist/tristan-ui.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "keywords": [
    "react",
    "design-system",
    "components",
    "ui",
    "typescript",
    "timeline",
    "complete"
  ],
  "author": "wenchuanlilizhao",
  "license": "MIT",
  "homepage": "https://github.com/wenchuanlilizhao/tristan-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenchuanlilizhao/tristan-ui.git",
    "directory": "react/packages/all"
  },
  "bugs": {
    "url": "https://github.com/wenchuanlilizhao/tristan-ui/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "vite build --mode library && tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "dev": "vite build --mode library --watch"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@tristan-ui/core": "^0.4.8",
    "@tristan-ui/timeline": "^0.4.8"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
} 